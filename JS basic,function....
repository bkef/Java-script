//function 3 - number
function Age(){
		age = parseInt(prompt("How old are you?"),10)+1;
		alert(" you will be "+age+" years next year");
}



//function 2 - number
function Number(){

let number = parseInt(prompt("gime me a number"),10);
let total = number + 10;
alert(total);

}



//Function 1 - User name
function Username(){

let name = prompt("hello, what is your name?");
document.getElementById("demo").innerHTML = "hello" + name;
}



